<?xml version="1.0" encoding="utf-8"?>
	<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
    <Type Name="doubleE10">
      <DisplayString>{{{mantisse} {int(exponent)}}}</DisplayString>
      <Expand>
        <CustomListItems>
          <Variable Name="POW" InitialValue="1"/>
          <Exec>POW = pow(mantisse,exponent)</Exec>
          <Item>(mantisse/POW),g</Item>
        </CustomListItems>
      </Expand>
    </Type>
</AutoVisualizer>